<section class="container mt-4">
  <h2>Kender Attendance Report for <%= date %></h2>
  <table class="table table-bordered">
    <thead>
      <tr>
        <th>#</th>
        <th>Saadhak Name</th>
        <th>Status</th>
      </tr>
    </thead>
    <tbody>
      <% attendanceRecords.forEach((record, index) => { %>
        <tr>
          <td><%= index + 1 %></td>
          <td><%= record.saadhak.name %></td>
          <td><%= record.status %></td>
        </tr>
      <% }) %>
    </tbody>
  </table>
</section>
